<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.jeecg.modules.srm.mapper.SupplierAccountMapper">
    <!-- 根据用户名查询 -->
    <select id="getUserByName" resultType="org.jeecg.modules.srm.entity.SupplierAccount">
        select * from  supplier_account  where username = #{username} and del_flag = 0
    </select>
</mapper>